    "css-loader": "^6.8.1",
    "cssnano": "^7.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
